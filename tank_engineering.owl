        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="ru">Толщина бронированноо листа.</rdfs:comment>
        <rdfs:label xml:lang="ru">Толщина</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#weight -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#weight">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Element"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Shell"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Tank"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="ru">Свойство изделия, выражающее его массу.</rdfs:comment>
        <rdfs:label xml:lang="ru">Масса</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topDataProperty -->

    <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#topDataProperty">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Gun"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ArmorPiercingShell -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ArmorPiercingShell">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Shell"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#HighExplosiveShell"/>
        <rdfs:comment xml:lang="ru">Бронебойный снаряд - снаряд, использующийся для пробития бронированных целей прямой наводкой.</rdfs:comment>
        <rdfs:label xml:lang="ru">Бронебойный снаряд</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ArmorSkirts -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ArmorSkirts">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ArmoredSheet"/>
        <rdfs:comment xml:lang="ru">Бронированные листы, обрамляющие ходовую часть танка сверху и уменьшающие шанс повреждения снарядом ходовой части почти при любой траектории полета.</rdfs:comment>
        <rdfs:label xml:lang="ru">Броня ходовой части</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ArmoredSheet -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ArmoredSheet">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Element"/>
        <rdfs:comment xml:lang="ru">Бронированный лист.</rdfs:comment>
        <rdfs:label xml:lang="ru">Бронированный лист</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Autoloader -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Autoloader">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Element"/>
        <rdfs:comment xml:lang="ru">Автомат заряжания - автоматическая система подачи, загрузки, заряжания и сбора отстрелянных снарядов.</rdfs:comment>
        <rdfs:label xml:lang="ru">Автомат заряжания</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Caliber -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Caliber">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Large"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Low"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Medium"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ValuePartition"/>
        <rdfs:comment xml:lang="ru">Калибр - величина, равная диаметру орудия.</rdfs:comment>
        <rdfs:label xml:lang="ru">Калибр</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ChainTrack -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ChainTrack">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Element"/>
        <rdfs:comment xml:lang="ru">Гусеничная лента - часть ходовой части, которая представляет собой замкнутую сплошную ленту из шарнирно закрепленных звеньев (чаще всего плоских листов из определенного материала).</rdfs:comment>
        <rdfs:label xml:lang="ru">Гусеничная лента</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#CompositeShell -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#CompositeShell">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ArmorPiercingShell"/>
        <rdfs:comment xml:lang="ru">Кумулятивный снаряд - снаряд, использующийся для пробития бронированных целей  и мелких препятствий прямой наводкой, использующий кумулятивный эффект (задание направления взрыва содержимого снаряда размещением выемки напротив детонатора, а не при нем).</rdfs:comment>
        <rdfs:label xml:lang="ru">Кумулятивный снаряд</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Cupola -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Cupola">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Element"/>
        <rdfs:comment xml:lang="ru">Командирская башенка - возвышающийся на крыше башни выступ с люком, вмещающий в себя приборы для наблюдения. Используется командиром экипажа для наблюдения близлежащей местности.</rdfs:comment>
        <rdfs:label xml:lang="ru">Комбашенка</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#DriveWheel -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#DriveWheel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Wheel"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#IdlerWheel"/>
        <rdfs:comment xml:lang="ru">Ведущее колесо, или зведочка - колесо-звездочка, которое располагается спереди и сзади гусеничной конструкции для приведения ее в движение в результате внешнего воздействия.</rdfs:comment>
        <rdfs:label xml:lang="ru">Ведущее колесо</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Element -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Element">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="ru">Общая концепция любого элемента, который может присутствовать в танке.</rdfs:comment>
        <rdfs:label xml:lang="ru">Деталь</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Engine -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Engine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Element"/>
        <rdfs:comment xml:lang="ru">Двигатель - устройство, преобразующее теплоту в механическую энергию.</rdfs:comment>
        <rdfs:label xml:lang="ru">Двигатель</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#FuelReservoir -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#FuelReservoir">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Element"/>
        <rdfs:comment xml:lang="ru">Топливные баки - полые области, расположенные чаще всего на внешней стороне корпуса танка, предназначенные для хранения и транспортироваки топлива.</rdfs:comment>
        <rdfs:label xml:lang="ru">Топливные баки</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#GlacisPlate -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#GlacisPlate">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ArmoredSheet"/>
        <rdfs:comment xml:lang="ru">Верхняя лобовая деталь - бронированный лист, являющийся основной частью бронирования корпуса танка. Чаще всего размещается под углом, близким к горизонтали, для повышения шанса рикошета, и имеет толщину больше, чем большинство деталей бронирования танка.</rdfs:comment>
        <rdfs:label xml:lang="ru">ВЛД</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Gun -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Gun">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Element"/>
        <rdfs:comment xml:lang="ru">Орудие - огнестрельное ствольное оружие, которое используется на силовых установках и не предназначено для ручной транспортировки. Применительно к танковому вооружению используются пушки прямой наводки с настильной траекторией полета снаряда.</rdfs:comment>
        <rdfs:label xml:lang="ru">Орудие</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#GunMantlet -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#GunMantlet">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ArmoredSheet"/>
        <rdfs:comment xml:lang="ru">Маска орудия - бронированный, чаще всего конусообразыно изогнутый лист, имеющий наибольшую толщину в сравнении с остальными деталями бронирования танка.</rdfs:comment>
        <rdfs:label xml:lang="ru">Маска</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#HeavyWeight -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#HeavyWeight">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#TankWeight"/>
        <rdfs:comment xml:lang="ru">Танки весом от 40 тонн.</rdfs:comment>
        <rdfs:label xml:lang="ru">Тяжелый</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#HighExplosiveAntiTankShell -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#HighExplosiveAntiTankShell">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#CompositeShell"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#HighExplosiveShell"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#HighExplosiveSquashHeadShell"/>
        <rdfs:comment xml:lang="ru">Кумулятивно-осколочный снаряд - снаряд, по конструкции не отличающийся от кумуляивного за исключением массы и толщины оболочки, предназначенный больше для поражения целей, а не нанесения урона в максимальном радиусе.</rdfs:comment>
        <rdfs:label xml:lang="ru">Кумулятивно-осколочный заряд</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#HighExplosiveShell -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#HighExplosiveShell">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Shell"/>
        <rdfs:comment xml:lang="ru">Осколочно-фугасный снаряд - снаряд, совмещающий осколочное и фугасное действие и предназначенный для поражения большого количества целей на максимально возможной территории: как живых, так и бронированных.</rdfs:comment>
        <rdfs:label xml:lang="ru">Осколочно-фугасный заряд</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#HighExplosiveSquashHeadShell -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#HighExplosiveSquashHeadShell">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#HighExplosiveShell"/>
        <rdfs:comment xml:lang="ru">Бронебойно-фугасный снаряд - снаряд, используемый для поражения бронетехники и укреплений с использованием метода пластичной деформации, условием которому является наличие тонких стенок оболочки снаряда.</rdfs:comment>
        <rdfs:label xml:lang="ru">Бронебойно-фугасный заряд</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Hull -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Hull">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Element"/>
        <rdfs:comment xml:lang="ru">Корпус танка - совокупность фронтальных, кормовых и боковых бронированных листов, которая образует защиту членов экипажа и большинства устройств танка.</rdfs:comment>
        <rdfs:label xml:lang="ru">Корпус</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#IdlerWheel -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#IdlerWheel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Wheel"/>
        <rdfs:comment xml:lang="ru">Ведомое колесо - колесо, расположенное между крайними - ведущими - колесами, используется для поддержания плавного хода гусеничной ленты.</rdfs:comment>
        <rdfs:label xml:lang="ru">Ведомое колесо</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#LargeCaliberGun -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#LargeCaliberGun">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Gun"/>
        <rdfs:comment xml:lang="ru">Орудие, имеющее крупный калибр.</rdfs:comment>
        <rdfs:label xml:lang="ru">Крупнокалиберное орудие</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#LightWeight -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#LightWeight">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#TankWeight"/>
        <rdfs:comment xml:lang="ru">Танки весом до 20 тонн.</rdfs:comment>
        <rdfs:label xml:lang="ru">Легкий</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#LowCaliberGun -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#LowCaliberGun">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Gun"/>
        <rdfs:comment xml:lang="ru">Орудие, имеющее малый калибр.</rdfs:comment>
        <rdfs:label xml:lang="ru">Малокалиберное орудие</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ManufactoringCompany -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#ManufactoringCompany">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="ru">Компания-производитель изделий.</rdfs:comment>
        <rdfs:label xml:lang="ru">Компания-производитель</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#MediumCaliberGun -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#MediumCaliberGun">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Gun"/>
        <rdfs:comment xml:lang="ru">Орудие, имеющее средний калибр.</rdfs:comment>
        <rdfs:label xml:lang="ru">Среднекалиберное орудие</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#MediumWeight -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#MediumWeight">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#TankWeight"/>
        <rdfs:comment xml:lang="ru">Танки весом 20 - 40 тонн.</rdfs:comment>
        <rdfs:label xml:lang="ru">Средний</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#OriginCountry -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#OriginCountry">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#America"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#England"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Germany"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Italy"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Japan"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Russia"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="ru">Страна, которая владеет компанией-производителем.</rdfs:comment>
        <rdfs:label xml:lang="ru">Страна-владелец</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#RadioTransceiver -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#RadioTransceiver">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Element"/>
        <rdfs:comment xml:lang="ru">Радиостанция - устройство, предназначенное для обмена информацией и наблюдения за местностью в допустимом радиусе.</rdfs:comment>
        <rdfs:label xml:lang="ru">Радиостанция</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Shell -->

    <owl:Class rdf:about="http://www.semanticweb.org/user/ontologies/2019/2/tank_engineering#Shell">
